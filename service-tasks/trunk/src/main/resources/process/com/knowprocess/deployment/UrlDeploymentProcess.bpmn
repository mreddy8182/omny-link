  <process id="urlDeploymentProcess" name="Url deployment process" isExecutable="true">
    <startEvent id="startevent1" name="Start" activiti:initiator="initiator"></startEvent>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Fetch resource" activiti:class="com.knowprocess.resource.spi.Fetcher"></serviceTask>
    <callActivity id="callDeploymentProcess" name="Call deployment process" calledElement="deploymentProcess">
      <extensionElements>
        <activiti:in source="resourceName" target="resourceName"></activiti:in>
        <activiti:in source="resource" target="resource"></activiti:in>
        <activiti:in source="initiator" target="initiator"></activiti:in>
        <activiti:in source="startInstance" target="startInstance"></activiti:in>
        <activiti:out source="errors" target="errors"></activiti:out>
        <activiti:out source="messages" target="messages"></activiti:out>
      </extensionElements>
    </callActivity>
    <sequenceFlow id="flow2" sourceRef="servicetask1" targetRef="callDeploymentProcess"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow3" sourceRef="callDeploymentProcess" targetRef="endevent1"></sequenceFlow>
    <textAnnotation id="textannotation1">
      <text>IN
---
resource: URL of resource to be loaded.
startInstance: true if an instance is to be started (provided deployment succeeds).</text>
    </textAnnotation>
