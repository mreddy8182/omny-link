      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!empty errors}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="fixProcess" targetRef="endevent2"></sequenceFlow>
    <serviceTask id="servicetask2" name="Start process" activiti:async="true" activiti:class="com.knowprocess.deployment.ProcessStarterService"></serviceTask>
    <sequenceFlow id="flow9" sourceRef="startevent1" targetRef="tweakProcess"></sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="validateProcess" targetRef="exclusivegateway1"></sequenceFlow>
    <endEvent id="endevent4" name="End"></endEvent>
    <sequenceFlow id="flow15" sourceRef="servicetask2" targetRef="endevent4"></sequenceFlow>
    <textAnnotation id="textannotation1">
